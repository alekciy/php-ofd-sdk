{
    "name": "alekciy/php-ofd-sdk",
    "description": "Библиотека работает с API операторов фискальных данных (ОФД)",
    "keywords": [
        "ОФД",
        "оператор фискальных данных",
        "ФФД",
        "формат фискальных документов",
        "taxcom.ru",
        "SDK"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Сундуков Алексей",
            "email": "public-mail@alekciy.ru"
        }
    ],
    "require": {
        "php": "~7.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.5",
        "vlucas/valitron": "^1.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "^6.5"
    },
    "config": {
        "platform": {
            "php": "7.0"
        },
        "sort-packages": true
    },
    "support": {
        "issues": "https://github.com/alekciy/php-ofd-sdk/issues",
        "source": "https://github.com/alekciy/php-ofd-sdk"
    },
    "autoload": {
        "psr-4": {
            "alekciy\\ffd\\": "src/ffd/",
            "alekciy\\ofd\\": "src/"
        }
    },
    "scripts": {
        "test-unit": "./vendor/bin/phpunit --colors=always"
    }
}
